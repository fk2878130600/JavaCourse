package HomeWork06;

public class Customer {
	private String ssn;
	private String name;
	private CheckingAccount cs;
	private SavingsAccount ss;
	
	public Customer(String ssn,String name,CheckingAccount cs,SavingsAccount ss){
		this.ssn = ssn;
		this.name = name;
		this.cs = cs;
		this.ss = ss;
	}	

	public String getSsn() {
		return ssn;
	}
	public void setSsn(String ssn) {
		this.ssn = ssn;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public CheckingAccount getCs() {
		return cs;
	}
	public void setCs(CheckingAccount cs) {
		this.cs = cs;
	}
	public SavingsAccount getSs() {
		return ss;
	}
	public void setSs(SavingsAccount ss) {
		this.ss = ss;
	}
	
	
	
}
