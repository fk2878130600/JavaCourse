package HomeWork05;

public class ClassList {
	private int semester;
	private int year;
	private Subject sub1;
	private Student s1;
	private Student s2;
	
	public ClassList(int semester,int year,Subject sub1,Student s1,Student s2){
		this.semester = semester;
		this.year = year;
		this.sub1 = sub1;
		this.s1 = s1;
		this.s2 =s2;
	}

	public int getSemester() {
		return semester;
	}

	public void setSemester(int semester) {
		this.semester = semester;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public Subject getSub1() {
		return sub1;
	}

	public void setSub1(Subject sub1) {
		this.sub1 = sub1;
	}

	public Student getS1() {
		return s1;
	}

	public void setS1(Student s1) {
		this.s1 = s1;
	}

	public Student getS2() {
		return s2;
	}

	public void setS2(Student s2) {
		this.s2 = s2;
	}

	public String toString() {
		String info = "";
		info = "semester=" + this.semester + ",year=" + this.year;
		info += "sub1:\n";
		info += this.sub1.toString() + "\n";
		info += "s1:\n";
		info += this.s1.toString() + "\n";
		info += "s2:\n";
		info += this.s2.toString() + "\n";
		return info;
	}
	
	
}

